    <head>
     <script src="http://code.highcharts.com/highcharts.js"></script>
        <title>redhat</title>
    </head>
    <body>
    <div id="charts" style="width:100%; height:400px;">
     <script>
        $(function () { 
    $('#charts').highcharts({
        chart: {
            type: 'bar'
        },
        title: {
            text: 'Fruit Consumption'
        },
        xAxis: {
            categories: ['Apples', 'Bananas', 'Oranges']
        },
        yAxis: {
            title: {
                text: 'Fruit eaten'
            }
        },
        series: [{
            name: 'Jane',
            data: [1, 0, 4]
        }, {
            name: 'John',
            data: [5, 7, 3]
        }]
    });
});
    </script>
    </body>  
    
